<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>{{t 'title'}}</th>
      <th>{{t 'actions'}}</th>
    </tr>
  </thead>
  <tbody>
    {{#each record}}
      <tr>
        <td>{{id}}</td>
        <td>{{title}}</td>
        <td>

        </td>
      </tr>
    {{else}}
      <tr>
        <td colspan="3">{{t 'certification.user.empty'}}</td>
      </tr>
    {{/each}}
  </tbody>
  <tfoot></tfoot>
</table>